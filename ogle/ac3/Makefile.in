top_builddir = ..

CC=@CC@
DFLAGS=@CFLAGS@ @DEFS@
LDFLAGS=@LIBS@
CFLAGS= -DDCFLAGS='"$(DFLAGS)"' -DDLDFLAGS='"$(LDFLAGS)"' $(DFLAGS)

all: ac3dec_wrap

